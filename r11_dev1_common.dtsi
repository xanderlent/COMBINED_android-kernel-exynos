			// if initial-mode 0 LDO23, LDO24 controlled by HRM_EN
			ldo23_reg: LDO23 {
				regulator-name = "vdd_ldo23";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3375000>;
				regulator-always-on;
				regulator-ramp-delay = <12000>;
