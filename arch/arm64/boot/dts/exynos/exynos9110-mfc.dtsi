			compatible = "samsung,exynos-mfc-core";
			id = <0>;
			reg = <0x0 0x12C80000 0x10000>;
			interrupts = <0 INTREQ__MFC IRQ_TYPE_LEVEL_HIGH>;
			clock-names = "aclk_mfc";
			clocks = <&clock CLK_MFCMSCLK_BASE>;
			iommus = <&sysmmu_mfcmscl>;

			samsung,tzmp;
			samsung,imgloader-s2mpu-support;

			/* MFC version */
			ip_ver = <0x1021>;

			/* Sysmmu check */
			share_sysmmu = <1>;
			axid_mask = <0x7>;
			mfc_fault_num = <0x0>;
			trans_info_offset = <0x1004>;

			/* LLC (Last Level Cache) */
			llc = <0>;
			need_llc_flush = <0>;

			/* vOTF */
			mfc_votf_base = <0x0>;
			gdc_votf_base = <0x0>;
			//dpu_votf_base = <0x0>;
			votf_start_offset = <0x0>;
			votf_end_offset = <0x0>;

			/* QoS */
			num_default_qos_steps = <9>;
			num_encoder_qos_steps = <8>;
			max_mb = <5315425>;
			max_hw_mb = <3888000>;
			mfc_freq_control = <1>;
			mo_control = <1>;
			bw_control = <1>;

			/* Device virtual address */
			#dma-address-cells = <1>;
			#dma-size-cells = <1>;
			dma-window = <0x0 0xD0000000>;

			/* Sub nodes for sysmmu, hwfc and mmcache */
			#address-cells = <2>;
			#size-cells = <1>;
			ranges;
			iommu {
			        reg = <0x0 0x12C50000 0x9000>;
			};

			/* Default QoS table */
			mfc_default_qos_table {
				mfc_d_qos_variant_0 {
					thrd_mb = <0>;
					freq_mfc = <178000>;
					freq_int = <80000>;
					freq_mif = <421000>;
					time_fw = <1474>;
