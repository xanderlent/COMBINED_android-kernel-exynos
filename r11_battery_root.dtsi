		thermal-sensors = <&tsmab104_ntc 0>;
	};
	batt_ntc: BATT_NTC {
		polling-delay = <10000>; /* milliseconds */
		polling-delay-passive = <0>; /* milliseconds */
		thermal-sensors = <&tsmab104_ntc 1>;
	};
	lte_pa_ntc: LTE_PA_NTC {
	};
	audio_ntc: AUDIO_NTC {
	};
	ppg_ntc: PPG_NTC {
	};
};
